{
  "root": true,
  "env": {
    "browser": true,
    "es2020": true,
    "jquery": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "impliedStrict": true,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "func-names": "off"
  }
}
